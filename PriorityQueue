import java.util.PriorityQueue;

public class MinPriorityQueue {
    public static void main(String[] args) {
        //  إنشاء PriorityQueue افتراضي (يعمل كـ Min-Heap)
        PriorityQueue<Integer> pq = new PriorityQueue<>();

        //  إضافة عناصر للطابور
        pq.add(50);
        pq.add(10);
        pq.add(30);
        pq.add(20);

        //  إزالة العناصر بالترتيب (الأصغر أولاً)
        while (!pq.isEmpty()) {
            //  poll() يزيل العنصر الأصغر ويعيده
            System.out.println(pq.poll());
        }
    }
}
