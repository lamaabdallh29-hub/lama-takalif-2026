public class ReverseArray {
    public static void main(String[] args) {
        int[] arr = {1, 2, 3, 4, 5};

        // ğŸ” Ø¹ÙƒØ³ Ø§Ù„Ù…ØµÙÙˆÙØ© Ø¨Ø§Ø³ØªØ®Ø¯Ø§Ù… Ù…Ø¤Ø´Ø±ÙŠÙ†
        for (int i = 0, j = arr.length - 1; i < j; i++, j--) {
            int temp = arr[i];
            arr[i] = arr[j];
            arr[j] = temp;
        }

        // âœ… Ø·Ø¨Ø§Ø¹Ø© Ø§Ù„Ù†ØªÙŠØ¬Ø©
        for (int num : arr) {
            System.out.print(num + " ");
        }
    }
}
